struct Point
	x int
	y int
end

def test_while(n: int) -> int do
	let i = 0
	let sum = 0
	while i < n then do
		sum = sum + i
		i = i + 1
	end
	sum
end

def fact(n: int) -> int
	if n <= 1 then 1 else n * fact(n - 1)

